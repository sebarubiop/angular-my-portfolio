{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "1e1de97b-a744-405a-8b5a-0397bb3d01ce"
  },
  "newProjectRoot": "projects",
  "projects": {
    "angular-portfolio": {
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.json"
            ],
            "index": "src/index.html",
            "main": "src/main.ts",
            "outputPath": "dist/angular-portfolio",
            "polyfills": ["zone.js", "@angular/localize/init"],
            "scripts": [],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles",
                "src/styles/abstracts"
              ]
            },
            "tsConfig": "tsconfig.app.json",
            "extraWebpackConfig": "webpack.config.js"
          },
          "configurations": {
            "development": {
              "extractLicenses": false,
              "namedChunks": true,
              "optimization": false,
              "sourceMap": true,
              "extraWebpackConfig": "webpack.config.js"
            },
            "production": {
              "aot": true,
              "extractLicenses": true,
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "serviceWorker": "ngsw-config.json",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "extraWebpackConfig": "webpack.config.js"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "buildTarget": "angular-portfolio:build:development",
            "extraWebpackConfig": "webpack.config.js"
          },
          "configurations": {
            "development": {
              "buildTarget": "angular-portfolio:build:development",
              "extraWebpackConfig": "webpack.config.js"
            },
            "production": {
              "buildTarget": "angular-portfolio:build:production",
              "extraWebpackConfig": "webpack.config.js"
            }
          },
          "defaultConfiguration": "development"
        },
        "serve-ssr": {
          "builder": "@angular/build:ssr-dev-server",
          "configurations": {
            "development": {
              "buildTarget": "angular-portfolio:build:development",
              "serverTarget": "angular-portfolio:server:development"
            },
            "production": {
              "buildTarget": "angular-portfolio:build:production",
              "serverTarget": "angular-portfolio:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "server": {
          "builder": "@angular/build:ssr",
          "configurations": {
            "development": {
              "outputHashing": "none",
              "sourceMap": true
            },
            "production": {
              "outputHashing": "media",
              "sourceMap": false
            }
          },
          "options": {
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular/localize:extract",
          "options": {
            "buildTarget": "angular-portfolio:build"
          }
        }
      },
      "i18n": {
        "sourceLocale": "en-US",
        "locales": {
          "es": {
            "translation": "src/locale/messages.es.xlf",
            "subPath": "es"
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {},
      "sourceRoot": "src"
    }
  },
  "version": 1
}